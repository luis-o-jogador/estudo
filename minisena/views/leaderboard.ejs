<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Leaderboard</title>
</head>
<body>
    <h1>Leaderboard</h1>
    <table>
        <thead>
            <tr>
                <th>Posição</th>
                <th>Nome</th>
                <th>Conta</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach((users, index) => { %>
                <tr>
                    <td><%= index + 1 %></td>
                    <td><%= users.name %></td>
                    <td><%= users.conta %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
